<div class="modal fade" id="vote-sent" data-backdrop="static" data-keyboard="false" tabindex="-1" aria-labelledby="staticBackdropLabel" aria-hidden="true">
  <div class="modal-dialog modal-dialog-centered">
    <div class="modal-content">
      <div class="modal-header">
        <h5 class="modal-title" id="staticBackdropLabel">{{ websiteData.websiteConfig.title }}</h5>
        <button type="button" class="btn-close" data-bs-dismiss="modal" aria-label="Close"></button>
      </div>
      <div class="modal-body">
            <div id="js-vote-sent" class="toast-body">
                Hello, world! This is a toast message.
            </div>
      </div>
    </div>
  </div>
</div>
<div class="toast-container position-fixed bottom-0 right-0 p-3" style="z-index: 5; right: 0; bottom: 0;">
    <div class="toast hide" id="connected" role="alert" aria-live="assertive" aria-atomic="true" data-delay="3000">
        <div class="toast-header">
            <div class="background-primary rounded mr-2" alt="..."></div>
            <strong class="mr-auto">Vos informations</strong>
            <button type="button" class="btn-close me-2 m-auto" data-bs-dismiss="toast" aria-label="Close"></button>
        </div>
        <div id="js-message-connected" class="toast-body">
            Hello, world! This is a toast message.
        </div>
    </div>
</div>